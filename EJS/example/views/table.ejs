<%- include('header') -%>
<%- include('navbar') -%>

    <div class="container">
        <h1>User Table</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>City</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.id %></td>
                        <td><%= user.name %></td>
                        <td><%= user.email %></td>
                        <td><%= user.city %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>